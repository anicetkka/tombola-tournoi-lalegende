<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="goldGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#FFD700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFA500;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="brownGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#8B4513;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#654321;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="16" cy="16" r="15" fill="url(#brownGradient)" stroke="#654321" stroke-width="1"/>
  
  <!-- Sack text -->
  <text x="8" y="12" font-family="serif" font-size="8" font-weight="bold" fill="url(#goldGradient)" stroke="#654321" stroke-width="0.3">
    <tspan font-size="10">S</tspan>ack
  </text>
  
  <!-- Grc text -->
  <text x="8" y="22" font-family="serif" font-size="6" font-weight="bold" fill="url(#goldGradient)">
    <tspan font-size="8">G</tspan><tspan font-size="5">r</tspan><tspan font-size="8">C</tspan>
  </text>
  
  <!-- Mini excavator in the C -->
  <g transform="translate(20, 18)" fill="#FFD700" stroke="#8B4513" stroke-width="0.2">
    <!-- Excavator body -->
    <rect x="0" y="2" width="3" height="1.5" rx="0.3"/>
    <!-- Excavator arm -->
    <rect x="3" y="2.2" width="2" height="0.5" rx="0.2"/>
    <!-- Excavator bucket -->
    <path d="M5 2.2 L6 1.8 L6 2.6 L5 2.8 Z"/>
    <!-- Excavator tracks -->
    <rect x="0.5" y="3.5" width="2" height="0.8" rx="0.3"/>
    <!-- Excavator cabin -->
    <rect x="0.8" y="1.5" width="1.4" height="1" rx="0.2"/>
  </g>
</svg>
